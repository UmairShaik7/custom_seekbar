<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.example.customseekbar.customseekbar.VerticalRangeSeekBar
        android:id="@+id/sb_vertical_3"
        android:layout_width="120dp"
        android:layout_height="222dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.031"
        app:rsb_gravity="bottom"
        app:rsb_indicator_background_color="@android:color/transparent"
        app:rsb_indicator_height="wrap_content"
        app:rsb_indicator_padding_bottom="5dp"
        app:rsb_indicator_padding_left="10dp"
        app:rsb_indicator_padding_right="10dp"
        app:rsb_indicator_padding_top="170dp"
        app:rsb_indicator_show_mode="alwaysShow"
        app:rsb_indicator_text_color="@color/colorAccent"
        app:rsb_indicator_text_size="24sp"
        app:rsb_indicator_width="wrap_content"
        app:rsb_max="10"
        app:rsb_mode="single"
        app:rsb_orientation="left"
        app:rsb_progress_color="@color/colorAccent"
        app:rsb_thumb_drawable="@drawable/oval_seekbar"
        app:rsb_thumb_height="40dp"
        app:rsb_thumb_width="40dp"
        app:rsb_tick_mark_mode="number"
        app:rsb_tick_mark_text_array="@array/mark1_10Array"
        app:rsb_tick_mark_text_color="@color/tick_mark_color"
        app:rsb_tick_mark_text_margin="20dp" />

    <com.example.customseekbar.customseekbar.VerticalRangeSeekBar
        android:id="@+id/sb_vertical"
        android:layout_width="120dp"
        android:layout_height="222dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.968"
        app:rsb_gravity="bottom"
        app:rsb_indicator_background_color="@android:color/transparent"
        app:rsb_indicator_height="wrap_content"
        app:rsb_indicator_padding_bottom="5dp"
        app:rsb_indicator_padding_left="10dp"
        app:rsb_indicator_padding_right="10dp"
        app:rsb_indicator_padding_top="170dp"
        app:rsb_indicator_show_mode="alwaysShow"
        app:rsb_indicator_text_color="@color/colorAccent"
        app:rsb_indicator_text_size="24sp"
        app:rsb_indicator_width="wrap_content"
        app:rsb_max="100"
        app:rsb_mode="single"
        app:rsb_orientation="left"
        app:rsb_progress_color="@color/colorAccent"
        app:rsb_thumb_drawable="@drawable/oval_seekbar"
        app:rsb_thumb_height="40dp"
        app:rsb_thumb_width="40dp"
        app:rsb_tick_mark_mode="number"
        app:rsb_tick_mark_text_array="@array/markArray"
        app:rsb_tick_mark_text_color="@color/tick_mark_color"
        app:rsb_tick_mark_text_margin="20dp" />


</androidx.constraintlayout.widget.ConstraintLayout>